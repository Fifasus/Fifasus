import random

numbers = [1, 2, 3, 4, 5, 6]
leader1 = random.choice(numbers)
leader2 = random.choice(numbers)
print(f"({leader1}, {leader2})")

#or

class Dice:
    def roll(self):
        first = random.randint(1, 6)
        second = random.randint(1, 6)
        return first, second

dice = Dice()
print(dice.roll())